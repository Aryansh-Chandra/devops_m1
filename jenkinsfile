pipeline{
    agent any
    stages{

       stage('package'){
            steps{

                bat 'npm install'
              }


         }
        stage('test'){
            steps{

               bat 'npm test' 
                   echo "No tests defined" 
                            }
                            }


        stage('build'){
            steps{
                bat 'npm run build'  }  }
     }

}
