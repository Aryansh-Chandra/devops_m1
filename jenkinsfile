pipeline{
    agent {docker 'node:6.3'}
    Stages{

        Stage('package'){
            Steps{
                bat 'npm install'
            }

        }
        Stage('test'){
            Steps{
               bat 'npm test' 
               echo "No tests defined"

            }

        }
        Stage('build'){
            Steps{
                bat 'npm run build' 
            }

        }  
        

    }

}
